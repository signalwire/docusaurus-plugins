{
  "name": "docusaurus-plugins-monorepo",
  "version": "1.0.0",
  "description": "A comprehensive collection of Docusaurus plugins",
  "private": true,
  "workspaces": [
    "packages/*",
    "website"
  ],
  "scripts": {
    "start": "yarn build:packages && yarn start:website",
    "start:website": "yarn workspace website start",
    "start:website:baseUrl": "yarn workspace website start:baseUrl",
    "dev": "yarn build:packages && yarn dev:website",
    "dev:website": "yarn workspace website start",
    "build": "yarn build:packages && yarn build:website",
    "build:packages": "lerna run build --stream",
    "build:website": "yarn workspace website run build",
    "build:website:fast": "yarn workspace website run build:fast",
    "prebuild:website": "yarn run build:packages",
    "serve": "yarn workspace website run serve",
    "serve:website": "yarn workspace website serve",
    "test": "lerna run test --stream",
    "test:ci": "lerna run test --stream",
    "test:packages": "lerna run test --stream",
    "watch": "lerna run watch --stream",
    "lint": "eslint . && lerna run lint --stream",
    "lint:fix": "eslint . --fix && lerna run lint:fix --stream",
    "lint:ci": "eslint . --quiet && lerna run lint --stream",
    "type-check": "lerna run type-check --stream",
    "clean": "lerna run clean --stream && rimraf lib .docusaurus",
    "format": "prettier --write . && lerna run format --stream",
    "format:check": "prettier --check . && lerna run format:check --stream",
    "postinstall": "yarn build:packages",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "canary": "yarn canary:version && yarn canary:publish",
    "canary:version": "changeset version --snapshot canary",
    "canary:publish": "changeset publish --tag canary",
    "changelog": "changeset status",
    "release": "yarn build:packages && yarn changeset:publish",
    "prerelease": "yarn build:packages && yarn test && yarn lint",
    "version": "changeset version && yarn install",
    "publish:check": "lerna run prepublishOnly --stream",
    "npm:check": "npm whoami",
    "npm:login": "npm login",
    "lerna": "lerna",
    "lerna:build": "lerna run build --stream",
    "lerna:test": "lerna run test --stream",
    "lerna:clean": "lerna run clean --stream",
    "workspace": "yarn workspace",
    "workspaces": "yarn workspaces",
    "lock:update": "npx --yes yarn-deduplicate"
  },
  "keywords": [
    "docusaurus",
    "plugins",
    "documentation",
    "markdown",
    "react"
  ],
  "author": "Devon White <devon.white@signalwire.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/signalwire/docusaurus-plugins.git"
  },
  "bugs": {
    "url": "https://github.com/signalwire/docusaurus-plugins/issues"
  },
  "homepage": "https://github.com/signalwire/docusaurus-plugins#readme",
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@docusaurus/utils": "^3.0.0",
    "@docusaurus/utils-validation": "^3.0.0",
    "@types/fs-extra": "^11.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "fs-extra": "^11.0.0",
    "jest": "^29.7.0",
    "lerna": "6.6.2",
    "lerna-changelog": "^2.2.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0",
    "yarn": ">=1.22.0"
  },
  "resolutions": {
    "tar": ">=6.2.1",
    "semver": ">=7.5.2",
    "@octokit/request-error": ">=5.1.1",
    "@octokit/request": ">=8.4.1",
    "@octokit/plugin-paginate-rest": ">=9.2.2",
    "webpack-dev-server": ">=5.2.1"
  }
}
